{
    "compilerOptions": {
        "target": "ES2015",
        "lib": ["ES2015", "DOM"],
        "types": ["cypress", "node"],
        "skipLibCheck": true,
        "strict": false,
        "esModuleInterop": true,
        "moduleResolution": "node"
    },
    "include": ["cypress/**/*.ts", "cypress.config.ts"],
    "exclude": ["node_modules"]
}