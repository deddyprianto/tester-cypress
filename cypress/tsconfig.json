{
    "compilerOptions": {
        "target": "ES2015",
        "lib": ["ES2015", "DOM"],
        "types": ["cypress", "node"],
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "strict": false,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "allowJs": true,
        "resolveJsonModule": true
    },
    "include": ["**/*.ts", "../cypress.config.ts"]
}

